{
  "manifest_version": 2,
  "homepage_url": "https://github.com/dodying/Bookmark-Sync",
  "name": "__MSG_extName__",
  "version": "0.0.3",
  "default_locale": "en",
  "description": "__MSG_extDesc__",
  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/logo-16.png",
      "48": "images/logo-48.png"
    },
    "default_title": "__MSG_extDesc__",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "open_in_tab": true,
    "page": "options.html"
  },
  "permissions": [
    "bookmarks"
  ]
}
