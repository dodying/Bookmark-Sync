{
  "manifest_version": 2,
  "homepage_url": "https://github.com/dodying/Bookmark-Sync",
  "name": "__MSG_extName__",
  "version": "0.0.3",
  "default_locale": "en",
  "description": "__MSG_extDesc__",
  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png"
  },
  "background": {
    "scripts": [
      "js/browser-polyfill.js",
      "js/background.js"
    ],
    "persistent": true
  },
  "options_ui": {
    "open_in_tab": false,
    "page": "options.html"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "alarms"
  ]
}
